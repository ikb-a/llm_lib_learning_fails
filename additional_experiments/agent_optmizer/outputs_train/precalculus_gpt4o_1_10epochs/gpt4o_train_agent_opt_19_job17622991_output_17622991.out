from sympy import symbols, Eq, solve, simplify, sympify, atan2, acos, sqrt, pi, Matrix, SympifyError

def solve_question_with_sympy(problem_description, variables):
    try:
        # Convert problem description
        problem_lower = problem_description.lower()
        symbols_list = symbols(variables)

        # Parsing for equations
        if '=' in problem_lower:
            eqs = problem_description.replace(' and ', ',').split(',')
            equations = []
            for eq in eqs:
                if '=' in eq:
                    lhs, rhs = eq.split('=')
                    eq_obj = Eq(simplify(sympify(lhs.strip())), simplify(sympify(rhs.strip())))
                    equations.append(eq_obj)
            solution = solve(equations, symbols_list, dict=True)
            return solution if solution else "No solution found."

        # Handle coordinate conversions
        if 'convert rectangular coordinates' in problem_lower:
            coords_str = problem_description.split('(')[1].split(')')[0]
            x, y, z = map(sympify, coords_str.split(','))
            rho = sqrt(x**2 + y**2 + z**2)
            theta = atan2(y, x)
            if theta < 0:
                theta += 2 * pi
            phi = acos(z / rho)
            return (rho, theta, phi)

        # Direct expression evaluation
        sym_expr = sympify(problem_description)
        solution = solve(sym_expr, symbols_list, dict=True)
        return solution if solution else "Solved without result."

    except SympifyError as e:
        return f"SympifyError occurred: {str(e)}"
    except Exception as e:
        return f"Error occurred: {str(e)}"
