from sympy import Matrix

def find_plane_equation_from_points(point1, point2, point3):
    # Convert points to sympy matrices
    point1 = Matrix([float(coor) for coor in point1])
    point2 = Matrix([float(coor) for coor in point2])
    point3 = Matrix([float(coor) for coor in point3])

    # Compute two vectors in the plane
    vector1 = point2 - point1
    vector2 = point3 - point1

    # Compute the normal vector to the plane
    normal_vector = vector1.cross(vector2)

    # Unpack normal vector
    A, B, C = normal_vector

    # Ax + By + Cz + D = 0, where (x, y, z) is a point on the plane to find D
    D = -A*point1[0] - B*point1[1] - C*point1[2]

    # Return the plane equation parameters
    return (A, B, C, D)
